{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/food/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/user/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/cart/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/order/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
